<h2>Entries</h2>
 <ul>
   <% for entry in @entries %>
     <li>
       <h4><%= entry["title"] %></h4>
       <p><%= entry["occurred_on"] %></p>
       <p><%= entry["description"] %></p>
     </li>
   <% end %>
 </ul>

 <p><a href="/entries/new?place_id=<%= @place["id"] %>">New Entry</a></p>
 <p><a href="/places">Back to All Places</a></p>

